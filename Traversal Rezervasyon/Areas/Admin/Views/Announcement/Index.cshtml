@model System.Collections.Generic.List<Traversal_Rezervasyon.Areas.Admin.Models.AnnouncementAddListViewModel>

@{
    Layout = "_AdminLayout";
}

<h1>Duyuru Listesi</h1>

<table class="table table-bordered">
    <tr>
        <th>#</th>
        <th>Başlık</th>
        <th>İçerik</th>
        <th>Detaylar</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Id</td>
            <td>@item.Title</td>
            <td>@item.Cotent</td>
            <td><a href="#" class="btn btn-outline-dark">Detaylar</a></td>
        </tr>
    }
</table>