@model IEnumerable<Traversal_Rezervasyon.Areas.Admin.Models.Exchange_Rate>

@{
    Layout = "_AdminLayout";
}

<h1>Booking API Kur Bilgileri</h1>

<table class="table table-bordered">
    <tr>
        <th>Kur Adı</th>
        <th>Kur Alış Oranı</th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>@item.currency</td>
            <td>@item.exchange_rate_buy</td>
        </tr>
    }
</table>